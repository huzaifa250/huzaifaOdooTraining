<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!-- sequence for movie-->

        <record id="sequence_film" model="ir.sequence">
            <field name="name">Movies Sequence</field>
            <field name="code">film.film</field>
            <field name="active">TRUE</field>
            <field name="prefix">Movie/%(range_year)s/%(range_month)s/</field>
            <field name="padding">6</field>
           <field name="company_id" eval="False"/><!--sequence will be common for all company-->
        </record>
    </data>
</odoo>