<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!-- sequence for movie-->

        <record id="sequence_film" model="ir.sequence">
             <field name="name">Movies Sequence</field>
             <field name="code">film.film</field>
             <field name="active">TRUE</field>
             <field name="prefix">Request/%(range_year)s/%(range_month)s/</field>
             <field name="padding">6</field>
             <field name="number_next">1</field>
             <field name="number_increment">1</field>
          </record>
    </data>
</odoo>